<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ui.screens.principal.PrincipalController">
    <center>
        <StackPane prefHeight="150.0" prefWidth="200.0">
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <StackPane prefHeight="150.0" prefWidth="200.0"/>
            </AnchorPane>
        </StackPane>
    </center>
    <top>
        <MenuBar fx:id="principalMenu">
            <Menu fx:id="menuView" mnemonicParsing="false" onAction="#menuClick" text="View">
                <MenuItem fx:id="menuItemArtists" mnemonicParsing="false" onAction="#menuClick" text="Artists"/>
                <MenuItem fx:id="menuItemAlbums" mnemonicParsing="false" onAction="#menuClick" text="Albums"/>
                <MenuItem fx:id="menuItemSongs" mnemonicParsing="false" onAction="#menuClick" text="Songs"/>
                <MenuItem fx:id="menuItemExit" mnemonicParsing="false" onAction="#exit" text="Exit"/>
            </Menu>
        </MenuBar>
    </top>
</BorderPane>