<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.screens.todospokemon.TodosPokemonsController">

    <ImageView fx:id="imgViewPokemon" fitHeight="197.0" fitWidth="203.0" layoutX="262.0" layoutY="86.0"
               pickOnBounds="true" preserveRatio="true"/>

    <Label fx:id="txtIdPokemon" layoutX="65.0" layoutY="108.0" text="Label"/>

    <Label fx:id="txtNamePokemon" layoutX="65.0" layoutY="133.0" text="Label"/>

    <ImageView fx:id="imgViewPokemonShiny" fitHeight="186.0" fitWidth="203.0" layoutX="500.0" layoutY="80.0"
               pickOnBounds="true" preserveRatio="true"/>

    <Button fx:id="getPokemon" layoutX="53.0" layoutY="283.0" mnemonicParsing="false" onMouseClicked="#showPokemon"
            text="getPokemon"/>

    <TextField fx:id="pokedexId" layoutX="19.0" layoutY="254.0"/>

    <Label fx:id="stat" layoutX="73.0" layoutY="165.0" text="Label"/>
</AnchorPane>
